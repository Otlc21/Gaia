@model HotelViewModel;

<div class="card shadow-none" id="basic_info">
	<div class="card-header">
		<div class="d-flex align-items-center justify-content-between">
			<h5 class="fs-18">@Localizer["Hotel Details"]</h5>
		</div>
	</div>
	<div class="card-body pb-1">
		<div class="row">
			<div class="col-md-12">
				<div class="mb-3">
					<label class="form-label">@Localizer["Hotel Name"]</label>
					<input type="text" class="form-control">
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Type"]</label>
					<select class="select">
						<option value="">@Localizer["Select"]</option>
						<option value="Hotels">@Localizer["Hotels"]</option>
						<option value="Villa">@Localizer["Villa"]</option>
						<option value="Apartment">@Localizer["Apartment"]</option>
						<option value="Guest House">@Localizer["Guest House"]</option>
						<option value="Lodge">@Localizer["Lodge"]</option>
					</select>
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Valid From"]</label>
					<div class="input-icon-end position-relative">
						<input type="text" class="form-control datetimepicker" placeholder="dd/mm/yyyy" value="02-05-2024">
						<span class="input-icon-addon">
							<i class="isax isax-calendar"></i>
						</span>
					</div>
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Valid Until"]</label>
					<div class="input-icon-end position-relative">
						<input type="text" class="form-control datetimepicker" placeholder="dd/mm/yyyy" value="02-05-2024">
						<span class="input-icon-addon">
							<i class="isax isax-calendar"></i>
						</span>
					</div>
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Rating"]</label>
					<select asp-for="Rating" class="select">
						<option value="0">Select</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
						<option value="5">5</option>
					</select>
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Available Spots"]</label>
					<input type="text" class="form-control">
				</div>
			</div>
			<div class="col-lg-4 col-md-6">
				<div class="mb-3">
					<label class="form-label">@Localizer["Price"]</label>
					<input type="text" class="form-control">
				</div>
			</div>
		</div>
	</div>
</div>
<div class="card shadow-none" id="location">
	<div class="card-header">
		<div class="d-flex align-items-center justify-content-between">
			<h5 class="fs-18">@Localizer["Location"]</h5>
		</div>
	</div>
	<div class="card-body pb-1">
		<div class="row">
			@* <div class="col-xl-3 col-md-6">
				<div class="mb-3">
					<label class="form-label">Country</label>
					<select class="select">
						<option>Select</option>
						<option>United States</option>
