					<input type="text" class="form-control">
				</div>
			</div>
			<div class="col-xl-3 col-md-6">
				<div class="mb-3">
					<label class="form-label">State</label>
					<input type="text" class="form-control">
				</div>
			</div>
			<div class="col-xl-3 col-md-6">
				<div class="mb-3">
					<label class="form-label">Zip Code</label>
					<input type="text" class="form-control">
				</div>
			</div> *@
			<div class="col-md-12">
				<div class="mb-3">
					<label class="form-label">@Localizer["Address"]</label>
					<input type="text" class="form-control">
				</div>
