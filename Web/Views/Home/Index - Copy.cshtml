<div class="container search-box">
    <ul class="nav nav-pills mb-3" id="searchTabs" role="tablist">
        <li class="nav-item" role="presentation">
            <button class="nav-link active" id="stays-tab" data-bs-toggle="pill" data-bs-target="#stays" type="button" role="tab">@Localizer["Hotel"]</button>
        </li>
        <li class="nav-item" role="presentation">
            <button class="nav-link" id="flights-tab" data-bs-toggle="pill" data-bs-target="#flights" type="button" role="tab">@Localizer["Flight"]</button>
        </li>
        <li class="nav-item" role="presentation">
            <button class="nav-link" id="cars-tab" data-bs-toggle="pill" data-bs-target="#cars" type="button" role="tab">@Localizer["Car"]</button>
        </li>
    </ul>

    <div class="tab-content" id="searchTabContent">
        <div class="tab-pane fade show active" id="stays" role="tabpanel">
            <form>
                <div class="form-container">
                    <div class="form-group">
                        <label>@Localizer["Destination"]</label>

                        <div class="input-icon">
                            <i class="fas fa-map-marker-alt"></i>
                            <input type="text" class="form-control" placeholder="@Localizer["Destination"]">
                        </div>
                    </div>
                    <div class="form-group">
                        <label>Check-in</label>
                        <div class="input-icon">
                            <i class="fas fa-calendar-alt"></i>
                            <input type="date" class="form-control" placeholder="Check-in">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>Check-out</label>
                        <div class="input-icon">
                            <i class="fas fa-calendar-alt"></i>
                            <input type="date" class="form-control" placeholder="Check-out">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["Travelers"]</label>
                        <div class="input-icon">
                            <i class="fas fa-user"></i>
                            <input type="number" class="form-control" placeholder="@Localizer["Travelers"]" min="1">
                        </div>
                    </div>
                    <div class="form-group">
                        <label></label>
                        <button type="button" class="btn-search">@Localizer["Search"]</button>
                    </div>

                </div>
            </form>
        </div>

        <div class="tab-pane fade" id="flights" role="tabpanel">
            <form>
                <div class="form-container">
                    <div class="form-group">
                        <label>@Localizer["From"]</label>
                        <div class="input-icon">
                            <i class="fas fa-map-marker-alt"></i>
                            <input type="text" class="form-control" placeholder="@Localizer["From"]">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["To"]</label>
                        <div class="input-icon">
                            <i class="fas fa-map-marker-alt"></i>
                            <input type="text" class="form-control" placeholder="@Localizer["To"]">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["Departure date"]</label>
                        <div class="input-icon">
                            <i class="fas fa-calendar-alt"></i>
                            <input type="date" class="form-control" placeholder="Departure date">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["Travelers"]</label>
                        <div class="input-icon">
                            <i class="fas fa-user"></i>
                            <input type="number" class="form-control" placeholder="@Localizer["Travelers"]" min="1">
                        </div>
                    </div>
                    <div class="form-group">
                        <label></label>
                        <button type="button" class="btn-search">@Localizer["Search"]</button>
                    </div>
                </div>

            </form>
        </div>

        <div class="tab-pane fade" id="cars" role="tabpanel">
            <form>
                <div class="form-container">
                    <div class="form-group">
                        <label>@Localizer["Pick-up location"]</label>
                        <div class="input-icon">
                            <i class="fas fa-map-marker-alt"></i>
                            <input type="text" class="form-control" placeholder="Pick-up location">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["Pick-up date"]</label>
                        <div class="input-icon">
                            <i class="fas fa-calendar-alt"></i>
                            <input type="date" class="form-control" placeholder="Pick-up date">
                        </div>
                    </div>


                    <div class="form-group">
                        <label>@Localizer["Drop-off location"]</label>
                        <div class="input-icon">
                            <i class="fas fa-map-marker-alt"></i>
                            <input type="text" class="form-control" placeholder="Drop-off location">
                        </div>
                    </div>

                    <div class="form-group">
                        <label>@Localizer["Drop-off date"]</label>
                        <div class="input-icon">
                            <i class="fas fa-calendar-alt"></i>
                            <input type="date" class="form-control" placeholder="Drop-off date">
                        </div>
                    </div>
                    <div class="form-group">
                        <label></label>
                        <button type="button" class="btn-search">@Localizer["Search"]</button>
                    </div>

                </div>
            </form>
        </div>
    </div>
</div>




